{
  "name": "shoppygo/maketplace-bundle",
  "type": "symfony-bundle",
  "description": "The first open source marketplace platform based on PrestaShop.org",
  "keywords": ["marketplace"],
  "homepage": "https://github.com/orgs/ShoppyGo/repositories",
  "license": "MIT",
  "authors": [
    {
      "name": "Bwlab of Luigi Massa",
      "email": "contact@bwlab.it"
    },
    {
      "name": "ShoppyGo community",
      "homepage": "https://contributors.prestashop.com/"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^4.4",
    "symfony/yaml": "^4.4"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.4"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ShoppyGo\\MaketplaceBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ShoppyGo\\MaketplaceBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "<4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
