{% extends '@PrestaShop/Admin/layout.html.twig' %}
{% set layoutTitle = 'Shipping cost'|trans({}, 'Admin.Marketplace.Shipping') %}

{% block content %}
    {{ form_start(form) }}
    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-header">
                    {{ 'Record' | trans({}, 'Admin.Marketplace.Shipping') }}
                </div>
                <div class="card-body">{{ form_rest(form) }}</div>
                <div class="card-footer">
                    <a href="{{ path('admin_marketplace_seller_shipping') }}" class="btn btn-outline-secondary">{{ 'Cancel'|trans({},'Admin.Actions') }}</a>
                        <button class="btn btn-primary float-right">{{ 'Save'|trans({},'Admin.Actions') }}</button>
                </div>
            </div>
        </div>
    </div>

    {{ form_rest(form) }}

    {{ form_end(form) }}
{% endblock %}